    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "immutable": "^4.3.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.21",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "winston": "^3.8.2"
  },
  "_moduleAliases": {
    "@app": "./build/src",
    "@test": "./build/test"
  }
